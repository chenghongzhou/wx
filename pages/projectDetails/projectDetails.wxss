.projectbox{
  width: 100%;
  overflow: hidden;
  background: #efeff4;
}
.projecmian{
    padding-left:30rpx;
    background: #fff;
}
.projecttitle{
  width: 100%;
  font-size: 34rpx;
  color: #383838;
  height: 76rpx;
  line-height: 76rpx;
  border-bottom:1px solid #f2f2f2;
  background: #fff;
}
.peojecttype{
  width: 100%;
  padding: 20rpx 0rpx;
  display: flex;
  display: -webkit-flex;
  font-size: 30rpx;
  border-bottom: 1px solid #f2f2f2;
}
.pt_name{
  color: #a5a5a5;
  width: 150rpx;
}
.pt_ms{
  color: #363636;
  word-break: break-all;
  padding-right: 26rpx;
}
.positioncion{
  display: inline-block;
  width: 24rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-left: 20rpx;
}
.peojectdetails{
  margin-bottom: 46rpx; 
  padding-right: 26rpx;
  font-size: 28rpx;
  color: #333;
}
.peojectdetails:last-child{
  padding-bottom: 43rpx;
}
.pj_tltie{
  font-size:28rpx;
  line-height:48rpx;
}
.talkbox{
  width: 100%;
  height: 130rpx;
  background: #fff;
    padding-top: 40rpx;
    position: fixed;
    bottom: 0;
}
.talkbnt{
  width: 80%;
  height: 96rpx;
  text-align: center;
  line-height: 96rpx;
  background: #4b92dc;
  font-size: 30rpx;
  color: #fff;
  margin: 0rpx auto;
  border-radius: 10rpx;
  box-shadow: 2rpx 6rpx 6rpx #c1cbd6;
}
.talkcion{
  width: 50rpx;
  height: 44rpx;
  vertical-align: middle;
  margin-right: 18rpx;
}